covidus_states <- read.csv("./us_states_covid19_daily.csv")
abb=covidus_states$state
region=abbr2state(abb)
covidus_states2 <- cbind(covidus_states,region)
covidus_states2 <- covidus_states2 %>%
filter(date == "20200323") %>%
mutate(positive = if_else(is.na(positive), 0, positive))
covidus_states2['fips'] <- fips(covidus_states2$state)
states=map_data("state")
states2 <- states %>%
group_by(region) %>%
summarise(lat = max(lat),long=max(long))
covidus_states2$region=tolower(covidus_states2$region)
usmap <- merge(covidus_states2, states2, by="region", all.x = T)
usmap$positive <- cut(usmap$positive,c(1,10,100,1000,10000,50000),
include.lowest = T,right = F)
plot_usmap(data = usmap,values="positive", labels=TRUE) +
scale_fill_manual(values=brewer.pal(5,"Reds"),
labels=c("<10","10-100","100-1000","1000-10000",">10000"),
name = "Positive Cases") +
labs(title = " Positive cases distribution of Covid-19 in US") +
dark_theme_void()
